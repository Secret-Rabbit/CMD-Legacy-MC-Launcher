@REM Maximum memory allocation pool for a Java Virtual Machine (JVM)
@REM Максимальный пул выделения памяти для виртуальной машины Java (JVM)
set RamXmx=2G
@REM Specifies the initial memory allocation pool.
@REM Задаёт начальный пул распределения памяти.
set RamXms=2G
@REM Preferred nickname
@REM Предпочитаемый ник
set MCNick=NickName
@REM Download environment at launch? (True/False)
@REM Загружать среду при запуске? (True/False)
set DownloadEnvironment=True
@REM Debugging mode (True/False)
@REM Режим отладки (True/False)
set Debugmode=False